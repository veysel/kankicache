<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>kankicache</PackageId>
    <Authors>veysel</Authors>
    <Company>kankicache</Company>
    <Description>Kanki Cache</Description>
    <Copyright>kankicache</Copyright>
    <PackageProjectUrl>https://github.com/veysel/kankicache</PackageProjectUrl>
    <RepositoryUrl>https://github.com/veysel/kankicache</RepositoryUrl>
    <PackageIcon>logo.png</PackageIcon>
    <PackageIconUrl />
    <PackageTags>cache kankicache kanki cache</PackageTags>
    <PackageReleaseNotes>Kanki Cache</PackageReleaseNotes>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\content\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="System.Runtime.Caching" Version="5.0.0" />
  </ItemGroup>

</Project>